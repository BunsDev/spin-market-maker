{
  "name": "spin-market-maker",
  "version": "0.1.0",
  "description": "Spin Market Maker example using Spin SDK",
  "type": "module",
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.68",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "~5.46.1",
    "@typescript-eslint/parser": "~5.46.1",
    "eslint": "~8.30.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-jest": "~27.1.7",
    "jest": "~29.3.1",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~29.0.3",
    "tsc-watch": "^6.0.0",
    "typescript": "~4.9.4"
  },
  "scripts": {
    "start": "NODE_ENV=production node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "dev": "tsc-watch -p ./tsconfig.json --onSuccess \"node ./build/src/main.js\"",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*\"",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@near-wallet-selector/core": "7.3.0",
    "@perk.money/perk-swap-core": "^0.2.85",
    "@ref-finance/ref-sdk": "^1.1.7",
    "@spinfi/core": "^2.1.0",
    "@spinfi/node": "^2.1.0",
    "@spinfi/shared": "^2.1.0",
    "bignumber.js": "^9.1.1",
    "bn.js": "^5.2.1",
    "ccxws": "^0.47.0",
    "convict": "^6.2.3",
    "dotenv-flow": "^3.2.0",
    "jsbi": "^4.3.0",
    "near-api-js": "^1.1.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "react": "^18.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "overrides": {
    "@ref-finance/ref-sdk": {
      "@near-wallet-selector/core": "~7.3.0"
    }
  },
  "volta": {
    "node": "16.13.0"
  }
}
