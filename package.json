{
  "name": "spin-grid-bot",
  "version": "0.0.1",
  "description": "Grid Bot example using Spin SDK",
  "type": "module",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/jest": "~27.4",
    "@types/node": "~16.11",
    "@typescript-eslint/eslint-plugin": "~5.14",
    "@typescript-eslint/parser": "~5.14",
    "eslint": "~8.11",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.1",
    "jest": "~27.5",
    "prettier": "~2.5",
    "rimraf": "~3.0",
    "ts-jest": "~27.1",
    "tsc-watch": "^5.0.3",
    "tsutils": "~3.21",
    "typescript": "~4.6"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "dev": "tsc-watch -p ./tsconfig.json --onSuccess \"node ./build/src/main.js\"",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@spinfi/node": "^0.1.7",
    "bignumber.js": "^9.0.2",
    "ccxws": "^0.47.0",
    "config": "^3.3.7",
    "convict": "^6.2.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.2.4",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "ts-convict": "^1.1.0",
    "tslib": "~2.3.1"
  },
  "volta": {
    "node": "16.13.0"
  }
}
